<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane prefHeight="250.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.InputDialogController">
   <children>
       <VBox fx:id="rootBox" alignment="CENTER" layoutX="-3.0" prefHeight="250.0" prefWidth="553.0" spacing="10" styleClass="container" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
           <padding>
               <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
           </padding>
          <children>
              <Label fx:id="HintString" alignment="CENTER" contentDisplay="CENTER" text="Enter Your New Expression:" textAlignment="CENTER" textOverrun="CLIP">
               <font>
                  <Font name="Arial Italic" size="26.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Label>
              <TextField fx:id="inputField" alignment="TOP_LEFT" opacity="0.7" prefHeight="42.0" prefWidth="462.0" promptText="Enter prefix expression">
               <font>
                  <Font size="21.0" />
               </font>
            </TextField>
              <Button onAction="#onSaveButtonClick" prefHeight="32.0" prefWidth="64.0" text="Save" />
          </children>
       </VBox>
   </children>
</AnchorPane>
